{
  "name": "MoverWarehouse",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.production react-native run-android",
    "ios": "ENVFILE=.env.production react-native run-ios --simulator='iPhone 8'",
    "ios-device": "ENVFILE=.env.production react-native run-ios --device='iPhone'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx",
    "pod-install": "npx pod-install ios",
    "android-release": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "code-push-ios": "appcenter codepush release-react -a moverarge-gmail.com/MOVER_WAREHOUSE-iOS -d Production",
    "code-push-android": "appcenter codepush release-react -a moverarge-gmail.com/MOVER_WAREHOUSE-Android -d Production",
    "fastlane-ios": "cd ios && bundle exec fastlane",
    "fastlane-android": "cd android && bundle exec fastlane"
  },
  "dependencies": {
    "@eva-design/eva": "2.0.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/drawer": "^5.9.3",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@ui-kitten/components": "5.0.0",
    "@ui-kitten/eva-icons": "5.0.0",
    "axios": "^0.20.0",
    "jsc-android": "^241213.1.0",
    "lottie-ios": "^3.1.8",
    "lottie-react-native": "^3.5.0",
    "moment": "^2.27.0",
    "react": "16.13.1",
    "react-hook-form": "^6.4.1",
    "react-native": "0.63.2",
    "react-native-animatable": "^1.3.3",
    "react-native-animated-numbers": "^0.1.1",
    "react-native-awesome-alerts": "^1.3.4",
    "react-native-barcode-mask": "^1.2.4",
    "react-native-camera": "3.40.0",
    "react-native-code-push": "6.3.0",
    "react-native-config": "^1.3.3",
    "react-native-firebase": "~5.6.0",
    "react-native-flash-message": "^0.1.16",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-keep-awake": "^4.0.0",
    "react-native-permissions": "^2.1.5",
    "react-native-qrcode-scanner": "^1.4.1",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.4",
    "react-native-screens": "^2.10.1",
    "react-native-sound": "^0.11.0",
    "react-native-svg": "^12.1.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-axios-middleware": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@ui-kitten/metro-config": "5.0.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.2",
    "react-test-renderer": "16.13.1",
    "reactotron-react-native": "^5.0.0"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": []
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react-native@1.0.23"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
